{
    "family": "explorer-frontend-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::075966805658:role/Explorer-ecs-staging",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "2048",
    "containerDefinitions": [
      {
        "name": "explorer-frontend-container",
        "image": "busybox",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3021,
            "hostPort": 3021
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/explorer-application/frontend",
            "awslogs-region": "ap-southeast-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
}
  